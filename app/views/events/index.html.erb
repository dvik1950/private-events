

  <table class="table">
    <thead>
    <tr>
      <th>Event name</th>
      <th>Event date</th>
      <th>Location</th>
    </tr>
    </thead>

    <tbody>
    <% @events.each do |e| %>
      <tr>
        <td><%= link_to e.name, event_path(e.id)%></td>
        <td><%=e.held_on%></td>
        <td><%="unknown"%></td>


      </tr>
    <% end %>
    </tbody>
  </table>
